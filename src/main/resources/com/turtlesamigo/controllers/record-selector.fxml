<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1"
         prefHeight="75" prefWidth="2048">
    <HBox prefHeight="46.0" prefWidth="600.0" spacing="20">
        <padding>
            <Insets left="20.0" right="20.0" top="20.0"/>
        </padding>
        <Label prefHeight="25.0" prefWidth="99.0" text="Train directory:"/>
        <TextField fx:id="_tfTrainRecordsDir" editable="false" prefHeight="25.0" prefWidth="340.0"/>
        <Button fx:id="_btnBrowse" prefHeight="25.0" prefWidth="99.0" text="Browse"/>
    </HBox>
    <HBox prefHeight="324.0" prefWidth="560.0" spacing="20.0">
        <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </VBox.margin>
        <TableView prefHeight="235.0" prefWidth="288.0" fx:id="_tvAllowedRadiologists">
            <columns>
                <TableColumn prefWidth="75.0" text="Show" fx:id="_tcRadiologistShow"/>
                <TableColumn prefWidth="75.0" text="Radiologist" fx:id="_tcRadiologistName"/>
            </columns>
            <HBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </HBox.margin>
        </TableView>
        <TableView prefHeight="235.0" prefWidth="273.0" fx:id="_tvAllowedFindingClasses">
            <columns>
                <TableColumn prefWidth="75.0" text="Show" fx:id="_tcFindingShow"/>
                <TableColumn prefWidth="75.0" text="Finding Class" fx:id="_tcFindingName"/>
            </columns>
            <HBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </HBox.margin>
        </TableView>
    </HBox>
</fx:root>
